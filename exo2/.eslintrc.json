{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["jest"],
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "no-var": "error",
    "prefer-const": "error"
  }
}
